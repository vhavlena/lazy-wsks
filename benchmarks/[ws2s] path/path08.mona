ws2s;

pred edge1(var2 X,Y) = ex2 Z: Z = X.0 & Z sub Y;
pred edge2(var2 X,Y) = ex2 Z: Z = X.1 & Z sub Y;
pred edge(var2 X,Y) = edge1(X,Y) | edge2(X,Y);

all2 Z1, Z2: ex2 X1, X2, X3, X4, X5, X6, X7, X8: edge(Z1, X1) & edge(X1, X2) & edge(X2, X3) & edge(X3, X4) & edge(X4, X5) & edge(X5, X6) & edge(X6, X7) & edge(X7, X8) & edge(X8, Z2);
